  "type": "library",
  "autoload": {
    "psr-4": {
      "Idynsys\\BillingSdk\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Idynsys",
      "email": "tech@idynsys.org"
    }
  ],
  "license": "MIT",
  "require": {
